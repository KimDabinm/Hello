<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- state_pressed: 버튼을 눌리거나 안 눌렸을 때 (true,false) -->
    <item android:state_pressed="false">
        <shape>
            <!-- solid : 배경색 지정하기 -->
            <solid android:color="#EAE7B1"></solid>
            <!-- corners : 사각형 모서리 부분을 둥글게 만듦-->
            <corners android:radius="10dp"></corners>
            <!-- stroke : 테두리 선 (width: 두께, color: 색-->
            <stroke android:width="3dp" android:color="#7D8F69"></stroke>
            <!-- size : 사이즈를 지정-->
            <size android:width="200dp" android:height="30dp"></size>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
        <solid android:color="#A6BB8D"></solid>
            <corners android:radius="10dp"></corners>
            <stroke android:width="2dp" android:color="#7D8F69"></stroke>
            <size android:width="200dp" android:height="30dp"></size>
        </shape>
    </item>

</selector>